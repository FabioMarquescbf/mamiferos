// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mamifero {
  id Int @id @default(autoincrement())
  nome String
  description String?
  mediaFilhotes Int
  mamifero Boolean
}

